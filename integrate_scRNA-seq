qc<-FeaturePlot(HFD3_2, features = c("log_nCount_RNA", "percent.mt"))
ggsave("/picb/bigdata/project/guoxin/ingradiatecdhfd/HFD3CONTENT/qc.pdf", qc, width = 14, height = 7)

# Plot markers
